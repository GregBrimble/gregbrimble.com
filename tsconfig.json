{
  "include": ["**/*"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2021"],
    "types": ["@cloudflare/workers-types", "@types/node"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2021",
    "strict": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "paths": {
      "~/*": ["./app/*"]
    },
    "noEmit": true
  }
}
